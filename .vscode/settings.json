{
  "cSpell.words": [
    "commitizen",
    "dateutil",
    "isort",
    "localtime",
    "nocontent",
    "odoo",
    "pycache",
    "Pyflakes",
    "Pylint",
    "pyproject",
    "relativedelta"
  ],
  "python.languageServer": "None",
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".vscode-insiders",
    "*log"
  ]
}
